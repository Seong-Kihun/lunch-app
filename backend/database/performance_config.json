{
  "performance_optimization": {
    "database": {
      "connection_pool_size": 20,
      "pool_recycle": 3600,
      "pool_pre_ping": true
    },
    "caching": {
      "redis_enabled": true,
      "cache_ttl": 300,
      "query_cache": true
    },
    "api": {
      "response_compression": true,
      "request_timeout": 30,
      "max_connections": 100
    },
    "memory": {
      "gc_threshold": 80,
      "memory_limit": "2GB"
    }
  }
}